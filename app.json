{
    "name": "Custom Webpack Config",
    "publicPath": "/",
    "buildDirectory": "dist",
    "entry": "./index.js",
    "htmlLocation": "./src/index.html",
    "assetsFolder": "assets",
    "scriptFolder": "js",
    "cssFolder": "css",
    "mediaFolder": "media",
    "manifest": {
        "generate": true,
        "filename": "manifest.json"
    },
    "ignoreMoment": true,
    "maxChunkSize": 300000,
    "devtool": "eval-cheap-source-map",
    "port": 3000
}
